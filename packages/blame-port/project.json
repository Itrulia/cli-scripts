{
  "root": "packages/blame-port",
  "sourceRoot": "packages/blame-port/src",
  "projectType": "library",
  "targets": {
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": ["packages/blame-port/**/*.ts"]
      }
    },
    "test": {
      "executor": "@nrwl/jest:jest",
      "outputs": ["coverage/packages/blame-port"],
      "options": {
        "jestConfig": "packages/blame-port/jest.config.ts",
        "passWithNoTests": true
      }
    }
  },
  "tags": []
}
